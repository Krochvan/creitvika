def setup():
    colorMode(HSB,360,100,100)
    size(1366,768)
    frameRate(10)
def draw():
    background(0)
    fill(random(0,360), 92, 98)
    for step in range(500):
        push()
        translate(random(0,1366),random(0,768))
        rotate(radians(random(0,360)))
        rect(random(0,1366),random(0,768),random(0,40),random(0,40),random(0,40))
        pop()
    textSize(100)
    fill("#FFFFFF")
    text("Happy New Year!!!",400,450)
